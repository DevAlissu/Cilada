  tra_obs                                                       String?                @db.VarChar(150)
  tra_status                                                    String?                @db.VarChar(1)
  tra_cancel                                                    String?                @db.VarChar(1)
  arm_it_transferencia                                          arm_it_transferencia[]
  cad_armazem_arm_transferencia_tra_arm_id_destinoTocad_armazem cad_armazem?           @relation("arm_transferencia_tra_arm_id_destinoTocad_armazem", fields: [tra_arm_id_destino], references: [arm_id], onDelete: NoAction, onUpdate: NoAction, map: "fkarm_transferencia_destino")
  cad_armazem_arm_transferencia_tra_arm_id_origemTocad_armazem  cad_armazem?           @relation("arm_transferencia_tra_arm_id_origemTocad_armazem", fields: [tra_arm_id_origem], references: [arm_id], onDelete: NoAction, onUpdate: NoAction, map: "fkarm_transferencia_origem")
}

model cad_armazem {

model arm_it_transferencia {
  itt_tra_id        Int
  itt_codprod       String            @db.VarChar(6)
  itt_qtd           Decimal?          @db.Decimal(38, 0)
  dbprod            dbprod            @relation(fields: [itt_codprod], references: [codprod])
  arm_transferencia arm_transferencia @relation(fields: [itt_tra_id], references: [tra_id])

  @@id([itt_tra_id, itt_codprod])
}

model arm_transferencia {
  tra_id                                                        Int                    @id(map: "pkarm_transferencia_id")
  tra_arm_id_origem                                             Int?
  tra_arm_id_destino                                            Int?
  tra_codusr_emissao                                            String?                @db.VarChar(4)
  tra_codusr_envio                                              String?                @db.VarChar(4)
  tra_codusr_recebimento                                        String?                @db.VarChar(4)
